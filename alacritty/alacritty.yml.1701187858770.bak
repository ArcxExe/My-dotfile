env:
  TERM: "xterm-256color"
window:
  opacity: 0.7
  decorations: none
scrolling:
  history: 1000
font:
  size: 13
  normal:
    family: "JetbrainsMono Nerd Font"
    style: "Bold"
cursor:
  style:
    shape: Block
    blinking: On
  unfocused_hollow: false
  blink_interval: 550
live_config_reload: true
colors:
  name: FarSide
  author: Baskerville
  primary:
    background: "#000000"
    foreground: "#919191"
  cursor:
    text: "#000000"
    cursor: "#919191"
  normal:
    black: "#303030"
    red: "#a43261"
    green: "#006ca5"
    yellow: "#007086"
    blue: "#6751a6"
    magenta: "#913e88"
    cyan: "#0061b1"
    white: "#c6c6c6"
  bright:
    black: "#5e5e5e"
    red: "#ff9fc9"
    green: "#3bd6ff"
    yellow: "#00ddf4"
    blue: "#d5b8ff"
    magenta: "#ffa7f6"
    cyan: "#93c9ff"
    white: "#ffffff"
theme: Baskerville - FarSide

