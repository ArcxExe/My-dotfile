env:
  TERM: "xterm-256color"
window:
  opacity: 0.7
  decorations: none
scrolling:
  history: 1000
font:
  size: 13
  normal:
    family: "JetBrainsMono Nerd Font"
    style: "Bold"

cursor:
  style:
    shape: Block
    blinking: On
  unfocused_hollow: false
  blink_interval: 550
live_config_reload: true


