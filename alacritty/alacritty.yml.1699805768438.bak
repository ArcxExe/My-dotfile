env:
  TERM: "xterm-256color"
window:
  opacity: 0.7
  decorations: none
scrolling:
  history: 1000
font:
  size: 13
  normal:
    family: "JetBrainsMono Nerd Font"
    style: "Bold"

cursor:
  style:
    shape: Block
    blinking: On
  unfocused_hollow: false
  blink_interval: 550
live_config_reload: true
items:
  - colors:
      name: Codeschool (dark)
      author: Chris Kempson
      primary:
        background: "#232c31"
        foreground: "#9ea7a6"
      cursor:
        text: "#232c31"
        cursor: "#9ea7a6"
      normal:
        black: "#232c31"
        red: "#2a5491"
        green: "#237986"
        yellow: "#a03b1e"
        blue: "#484d79"
        magenta: "#c59820"
        cyan: "#b02f30"
        white: "#9ea7a6"
      bright:
        black: "#3f4944"
        red: "#2a5491"
        green: "#237986"
        yellow: "#a03b1e"
        blue: "#484d79"
        magenta: "#c59820"
        cyan: "#b02f30"
        white: "#b5d8f6"
  - theme: Codeschool.dark
